<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geometry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Geometry">
        <activity android:name=".EllipseActivity"></activity>
        <activity android:name=".SectorCircleActivity" />
        <activity android:name=".AnnulusActivity" />
        <activity android:name=".SemiCircleActivity" />
        <activity android:name=".CircleActivity" />
        <activity android:name=".PolygonActivity" />
        <activity android:name=".IosRightTriangleActivity" />
        <activity android:name=".RightTriangleActivity" />
        <activity android:name=".EquilateralTriangleActivity" />
        <activity android:name=".GeneralTriangleActivity" />
        <activity android:name=".TrapeziumActivity" />
        <activity android:name=".RhombusActivity" />
        <activity android:name=".ParallelogramActivity" />
        <activity android:name=".SquareActivity" />
        <activity android:name=".RectangleActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>